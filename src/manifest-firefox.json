{
  "manifest_version": 2,
  "name": "Manga Mark",
  "description": "Mark your reading manga and store to server",
  "version": "1.0.30",
  "content_scripts": [
    {
      "matches": [
        "*://*/doc-*-chuong-*",
        "*://*/truyen-tranh/*/chap*/*",
        "*://saytruyen.net/truyen-*/chuong-*",
        "https://*/manga/*/chapter-*/",
        "https://*/chapter/*/chapter_*",
        "https://*/*-chap-*/*",
        "https://truyenz.info/manga/*/chuong-*/",
        "https://www.webtoon.xyz/read/*/chapter-*/",
        "https://www.webtoons.com/*/*/*/*/viewer?title_no=*&episode_no=*",
        "*://h5.mangatoon.mobi/*",
        "https://tienycomic.xyz/manga/*/chap-*",
        "https://*/doc-truyen/*",
        "https://*/*/chapter-*",
        "https://readmanganato.com/manga-*/chapter-*",
        "https://*.doctruyen3qk.com/*"
      ],
      "css": [
        "css/positions.css",
        "vendor/css/animate.min.css",
        "css/content.css"
      ],
      "js": [
        "vendor/js/sweetalert2.all.min.js",
        "js/content.js",
        "js/blockads.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/websites.js",
      "js/background.js"
    ]
  },
  "options_ui": {
    "page": "html/options.html",
    "open_in_tab": true
  },
  "browser_action": {
    "default_icon": "icons/favicon.png",
    "default_popup": "html/popup.html",
    "default_title": "Mark your manga"
  },
  "icons": {
    "128": "icons/favicon.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "alarms",
    "https://mangamark.herokuapp.com/"
  ]
}